
CREATE TABLE ARTIINF (
  ARTICLE_INFO_ID FOR COLUMN ARID       CHAR(6)
     CCSID 297 NOT NULL DEFAULT '' ,
  ARTICLE_INFORMATION FOR COLUMN ARTINF     VARCHAR(1520)
      CCSID 297 NOT NULL DEFAULT '' )
  ;

LABEL ON TABLE ARTIINF
  IS 'Article Informations File' ;

LABEL ON COLUMN ARTIINF
( ARTICLE_INFO_ID IS 'ARTICLE ID' ,
  ARTICLE_INFORMATION IS 'ARTICLE INFORMATION' ) ;

